<Window x:Class="SBMessenger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SBMessenger"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button1" Content="Login1" HorizontalAlignment="Left" Margin="113,80,0,0" VerticalAlignment="Top" Width="75" Click="button_Click_Login"/>
        <Button x:Name="button2" Content="Login2" HorizontalAlignment="Left" Margin="160,120,0,0" VerticalAlignment="Top" Width="75" Click="button_Click_Disconnect"/>
        <Button x:Name="button3" Content="SendMessage" HorizontalAlignment="Left" Margin="250,160,0,0" VerticalAlignment="Top" Width="75" Click="button_Click_ShowActiveUses"/>
        <Button x:Name="button" Content="Disconnect" HorizontalAlignment="Left" Margin="384,232,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
